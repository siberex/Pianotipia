{
  "name": "piano-board",
  "version": "1.0.0",
  "description": "Web component for displaying piano keyboard",
  "main": "piano-board.js",
  "module": "piano-board.js",
  "repository": "git@github.com:siberex/piano-board.git",
  "keywords": [
    "piano",
    "piano keyboard",
    "music",
    "web component",
    "lit-element",
    "typescript"
  ],
  "author": "Stepan Legachev <piano-board@sib.li>",
  "license": "MIT",
  "scripts": {
    "analyze": "wca analyze 'src/**/*.ts' --outFile custom-elements.json",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "lit-analyzer && eslint 'src/**/*.ts'",
    "format": "prettier src/* --write",
    "start:dev": "es-dev-server"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "es-dev-server": "^1.36.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "lit-analyzer": "^1.1.9",
    "prettier": "^1.19.1",
    "rollup": "^1.30.1",
    "ts-lit-plugin": "^1.1.9",
    "typescript": "^3.7.5",
    "web-component-analyzer": "^1.0.2"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "lit-element": "^2.2.1"
  }
}
